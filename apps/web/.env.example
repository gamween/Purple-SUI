# Sui Network Configuration
VITE_SUI_NETWORK=devnet
# Options: devnet, testnet, mainnet

# zkLogin OAuth Configuration
# Google OAuth
VITE_GOOGLE_CLIENT_ID=your-google-client-id.apps.googleusercontent.com

# Facebook OAuth
VITE_FACEBOOK_CLIENT_ID=your-facebook-app-id

# Twitch OAuth
VITE_TWITCH_CLIENT_ID=your-twitch-client-id

# OAuth Redirect URI (must match OAuth app configuration)
VITE_REDIRECT_URI=http://localhost:5173/auth/callback

# API Backend
VITE_API_URL=http://localhost:3001

# zkLogin Configuration
# Prover Service URL (for zkLogin proof generation)
VITE_ZKLOGIN_PROVER_URL=https://prover-dev.mystenlabs.com/v1

# Salt Service URL (optional, for deterministic address generation)
VITE_ZKLOGIN_SALT_SERVICE_URL=https://salt.api.mystenlabs.com/get_salt
